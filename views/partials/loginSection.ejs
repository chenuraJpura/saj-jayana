    <div class="row">
        
        <form method="POST" action="/users/login" class="content-wrapper mt-3" autocomplete="on">
            <h3 class="mt-4" style="font-family:'Aladin', cursive;font-size: xx-large;">Saj Jayana</h3>
            <h4 class="d-inline text-left">Login</h4>
            <div class="form-group mt-4">
                <input type="email" class="form-control " placeholder="Your Email *" name="email" />
            </div>
            <div class="form-group mt-4">
                <input type="password" class="form-control" placeholder="Your Password *" name="password"/>
            </div>
            <%- include('messages'); %> 
            <div class="form-group mt-4 button-wrapper-right" >
                <a href="/forget" class="ForgetPwd">Forget Password?</a>
            </div>
            <div class="form-group mt-4 button-wrapper-right">
                <button type="submit" class="btn btn-dark px-5">Login</button>
            </div>
            
        </form>
  
    </div>
  